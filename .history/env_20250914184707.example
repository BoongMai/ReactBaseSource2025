# Application Configuration
VITE_APP_NAME=BaseSource
VITE_APP_SHORT_NAME=RentPay
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Rent Payment Management App

# API Configuration
VITE_API_URL=https://api.example.com
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=production
VITE_APP_ENV=production

# Analytics
VITE_ANALYTICS_ID=your-analytics-id
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Security
VITE_CSRF_TOKEN=your-csrf-token
VITE_ENCRYPTION_KEY=your-encryption-key

# Features
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false

# External Services
VITE_SENTRY_DSN=your-sentry-dsn
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Cookie Configuration
VITE_COOKIE_DOMAIN=.example.com
VITE_COOKIE_SECURE=true

# Web URLs
VITE_WEB_URLS=https://example.com,https://www.example.com
VITE_ENV_WEB_APP_URL=https://example.com
